.subList{
  text-align: center;
  padding: 20rpx;
  box-sizing: border-box;
}
.subBox{
  background-color:#ffffff;
  border: 1px solid #e5e5e5;
  padding: 10rpx;
  border-radius:10rpx; 
  margin-bottom: 20rpx;
}
.subBox .topBar{
  padding: 20rpx 0;
  height: 70rpx;
  position: relative;
  text-align: left;
}
.subBox .topBar .photo{
  width: 70rpx;
}
.subBox .topBar .photo image{
  width: 70rpx;
  height: 70rpx;
  border-radius:100rpx; 
}
.subBox .topBar .simInfo text{
  display: block;
}
.subBox .topBar .simInfo{
  position: absolute;
  top: 20rpx;
  left:0rpx;
  right: 0;
}
.subBox .topBar .simInfo .companyName{
  font-size: 28rpx;
}
.subBox .topBar .simInfo .date{
  font-size: 20rpx;
}
.articleDesc{
  text-align: left;
  font-size: 24rpx;
    color: #999;
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    margin:0 0 20rpx 0;
}
.wFull{
  width: 100%;
  display: block;
}
.bottomBar{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  height: 75rpx;
}
.bottomBar view{
  margin-left: 20rpx;
  margin-top: 15rpx;
  font-size: 24rpx;
  padding: 5rpx 15rpx;
  color: #666666;
}
.bottomBar view:last-child{
  margin-left: 0;
  margin-right: 20rpx;
}
.reply{
  border-radius:10rpx;
  border: 1px solid #e5e5e5; 
}
.thumbs{
   border-radius:10rpx;
  border: 1px solid #e5e5e5; 
}
.region{
   border-radius:5rpx;
  border: 1px solid #e5e5e5; 
  background-color: #e5e5e5;
}
.type{
  text-align: center;
  margin: 0 0 20rpx 0;
}
.type text{
  vertical-align: middle;
  font-size: 30rpx;
  color: #ff0000;
}
.type image{
  width: 64rpx;
  height: 65rpx;
  vertical-align: middle;

}